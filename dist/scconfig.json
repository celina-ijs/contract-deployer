{
    "main": "@scom/dapp",
    "assets": "@modules/assets",
    "moduleDir": "modules",
    "modules": {
        "@modules/assets": {
            "path": "assets",
            "dependencies": []
        },
        "@modules/module1": {
            "path": "module1",
            "dependencies": [
                "@ijstech/eth-wallet",
                "@scom/scom-code-editor"
            ]
        }
    },
    "dependencies": {
        "@modules/assets": "*",
        "@ijstech/eth-wallet": "*",
        "@ijstech/eth-contract": "*",
        "@scom/scom-network-list": "*",
        "@scom/scom-multicall": "*",
        "@scom/dapp": "*",
        "@scom/scom-code-editor": "*"
    },
    "menus": [
        {
            "caption": "Contract Deploy",
            "url": "/deployer",
            "module": "@modules/module1",
            "params": {},
            "menus": []
        }
    ],
    "routes": [
        {
            "url": "/deployer",
            "module": "@modules/module1",
            "default": true
        }
    ],
    "wallets": [
        {
            "name": "metamask"
        }
    ],
    "networks": "*",
    "themes": {
        "default": "dark",
        "dark": {
            "background": {
                "modal": "#050120"
            },
            "colors": {
                "primary": {
                    "main": "#2862BB"
                }
            }
        }
    },
    "copyright": "&#169; 2022 IJS Technologies. All rights reserved.",
    "version": "0.1.14",
    "footer": {
        "visible": false
    },
    "header": {
        "customStyles": {
            "$nest": {
                "#imgDesktopLogo": {
                    "display": "none"
                },
                "#menuDesktop": {
                    "border": "none"
                }
            }
        }
    },
    "packages": [
        "@ijstech/components",
        "@ijstech/eth-contract",
        "@scom/scom-network-list",
        "@ijstech/eth-wallet",
        "@scom/scom-code-editor",
        "@scom/dapp",
        "@scom/scom-multicall"
    ]
}